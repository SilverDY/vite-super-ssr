{
  "name": "vite-super-ssr",
  "author": "Dmitrii Serebrov",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.16.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "start": "nodemon --watch src/app/server.ts --watch src/app/entries/server.tsx --exec 'yarn node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/app/server.ts'",
    "start:client": "vite --config vite.config.ts dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --resolve-plugins-relative-to \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint src --resolve-plugins-relative-to \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "moduleRoots": [
    "node_modules",
    "src"
  ],
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.9.2",
    "@types/express": "^4.17.13",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "nodemon": "^2.0.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.5",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.4",
    "reset-css": "^5.0.1",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-svgr": "^2.2.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.5",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.7.2",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-extra": "^0.1.0",
    "eslint-plugin-sort-exports": "^0.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-aliases": "^0.9.2",
    "vite-plugin-eslint": "^1.7.0"
  }
}
